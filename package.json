    "http-proxy-middleware": "^0.15.0",
    "immutable": "^3.8.1"
  },
  "devDependencies": {
    "cpx": "^1.3.1",
    "cross-env": "^1.0.7",
    "express": "^4.13.4",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11"
  },
  "peerDependencies": {
    "marked": "^0.3.5",
    "normalize.css": "^4.1.1",
    "react": "^15.0.1",
    "react-dev-deps": "~1.0.2",
    "react-dom": "^15.0.1",
    "react-modal": "^1.2.1",
    "react-redux": "^4.4.5",
